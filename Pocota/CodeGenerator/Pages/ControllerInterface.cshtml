@page
@using Net.Leksi.Pocota.Common
@model ClassModel
@Html.Partial("Header", Model)

public interface @Model.ClassName : @Html.Raw(string.Join(", ", Model.Interfaces))
{
    @foreach (MethodModel mm in Model.Methods)
    {
        <text>    [ExpectedOutputType(typeof(@Html.Raw(@mm.ExpectedOutputType)))]
    @mm.ReturnType @mm.Name@Html.Raw($"(")@Html.Raw(string.Join(", ", mm.Parameters.Select(v => $"{v.Type} {v.Name}"))));

</text>
    }
}
