@using Net.Leksi.Pocota.Common;
@model ClassModel

    @Html.Raw("#region Properties accessors");

    @foreach (PropertyModel pm in Model.Properties)
    {    
        <text>    private static object? @Html.Raw($"Get{pm.Name}Value(PocoBase target)")
    {
        return @Html.Raw($"(({Model.ClassName})target).{pm.Name}");
    }

        @if(!pm.IsList)
        {
            <text>    private static void @Html.Raw($"Set{pm.Name}Value(PocoBase target, object? value)")
    {
        @Html.Raw($"(({Model.ClassName})target).{pm.Name} = ({pm.Type})value!");
    }
</text>
        }
</text>
    }

    @Html.Raw("#endregion Properties accessors");

