  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Pocota\Common\CommonDebug.csproj" />
    <ProjectReference Include="..\..\Pocota\ContractImplementer\ContractImplementerDebug.csproj" />
