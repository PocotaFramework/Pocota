@page
@using Net.Leksi.Pocota.Pipeline
@model ClassModel
@foreach (string usng in Model.Usings.OrderBy(v => v))
{
    <text>using @usng;
</text>
}
@if(Model.Node.Namespace is { })
{
    <text>namespace @Model.Node.Namespace;

</text>
}
public abstract class @Model.Node.Name@if (Model.Node.Parent is { })
{
    <text>: @Model.Node.Parent.Name</text>
}

{
@foreach(PropertyHolder ph in Model.Node.Properties)
{
    <text>    public @ph.TypeName @ph.Name { get; set; }
</text>
}

}